{
  "editor.fontSize": 16,
  "editor.fontFamily": "FiraCode Nerd Font, Material Icons",
  "editor.formatOnSave": true,
  "editor.bracketPairColorization.enabled": true,
  "editor.fontLigatures": true,
  "[typescript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "redhat.telemetry.enabled": false,
  "latex-workshop.view.pdf.viewer": "tab",
  "[json]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[c]": {
    "editor.formatOnSave": false
  },
  "rust-analyzer.checkOnSave.overrideCommand": null,
  "git.enableSmartCommit": true,
  "git.enableCommitSigning": true,
  "editor.cursorStyle": "line-thin",
  "glassit.alpha": 248,
  "typescript.updateImportsOnFileMove.enabled": "always",
  "workbench.editor.untitled.hint": "hidden",
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[dockerfile]": {
    "editor.defaultFormatter": "ms-azuretools.vscode-docker"
  },
  "terminal.integrated.cursorStyle": "underline",
  "terminal.integrated.fontFamily": "FiraCode Nerd Font",
  "terminal.integrated.tabs.title": "${sequence}: ${process}",
  "explorer.confirmDelete": false,
  "files.exclude": {
    "**/.classpath": true,
    "**/.project": true,
    "**/.settings": true,
    "**/.factorypath": true
  },
  "files.associations": {
    "*.css": "css"
  },
  "explorer.fileNesting.enabled": true,
  "explorer.fileNesting.patterns": {
    "*.js": "${capture}.js.map, ${capture}.min.js, ${capture}.d.ts",
    "*.jsx": "${capture}.js",
    "*.tsx": "${capture}.ts",
    "tsconfig.json": "tsconfig.*.json",
    "package.json": "package-lock.json, yarn.lock",
    "*.ts": "${capture}.test.ts"
  },
  "dart.sdkPath": "/home/woomy/.dart",
  "Lua.telemetry.enable": false,
  "workbench.iconTheme": "material-icon-theme",
  "[dart]": {
    "editor.formatOnSave": true,
    "editor.formatOnType": true,
    "editor.rulers": [80],
    "editor.selectionHighlight": false,
    "editor.suggest.snippetsPreventQuickSuggestions": false,
    "editor.suggestSelection": "first",
    "editor.tabCompletion": "onlySnippets",
    "editor.wordBasedSuggestions": false
  },
  "[shellscript]": {
    "editor.defaultFormatter": "mkhl.shfmt",
    "editor.formatOnSave": false
  },
  "shfmt.executablePath": "/home/woomy/go/bin/shfmt",
  "workbench.colorTheme": "GitHub Dark",
  "telemetry.telemetryLevel": "off",
  "python.formatting.provider": "black",
  "window.zoomLevel": -1
}
