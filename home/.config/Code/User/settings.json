{
    "workbench.colorTheme": "Monokai Vibrant",
    "workbench.iconTheme": "Monokai Pro (Filter Octagon) Icons",
    "editor.fontSize": 16,
    "editor.fontFamily": "Iosevka",
    "editor.formatOnSave": true,
    "[typescript]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
